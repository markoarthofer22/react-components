        "import-from": "^4.0.0",
        "jest": "^27.0.4",
        "lerna": "^4.0.0",
        "lint-staged": "^11.1.2",
        "moment": "^2.29.1",
        "postcss": "^8.3.6",
        "prettier": "^2.2.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-ga": "^3.3.0",
        "react-router-dom": "^5.2.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.56.3",
